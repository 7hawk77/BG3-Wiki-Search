{
    "name": "Hard-Redirect",
    "description": "Hard coded redirects",
    "author": "DAFF0D11",
    "manifest_version": 3,
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "declarativeNetRequest"
    ],
    "action": {
        "default_icon": "icons/favicon.png",
        "default_title": "BG3 Wiki Search",
        "default_popup": "ui/ui.html"
    },
	"commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Alt+W"
            }
        }

    "host_permissions": [
        "<all_urls>"
    ]
}
